<h1 align="center">Listing All Categories</h1>
<%= @categories.each do |category| %>
	<ul class="listing" >
		<div class="row">
			<div class="well col-md-4 col-md-offset-4">
				<li class="article-title" style="none">
					<%= link_to "#{category.category_name}", category_path(category) %>
				</li>
			</div>
		</div>
	</ul>
	<% end %>