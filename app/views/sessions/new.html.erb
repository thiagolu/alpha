<div class="container">
  
  <div class="row">
    <div class="container">
      
      <h1>Fa√ßa o Login</h1>
    </div>
  </div>

  <div class="conatiner">
    <div class="col-xs-12">
      <%= form_for( :session, :html => { class: "form-horizontal", role: "form" }, url: login_path ) do |f| %>

      <div class="row">
        <br/>
        <div class="form-group">
          <div class="control-label-col-sm-2">
            <%= f.label :email %> 
          </div>
          <div class="col-sm-8">

            <%= f.email_field :email, class: "form-control", placeholder: "Valid@email.address", autofocus: true  %>
          </div>
        </div>
        <div class="form-group">
          <div class="control-label-col-sm-2">
            <%= f.label :password %> 
          </div>
          <div class="col-sm-8">

            <%= f.password_field :password, class: "form-control", placeholder: "EAS@***///|**", autofocus: true  %>
          </div>

        </div>
        <div class="form-group">
          <div class="col-sm-10">

            <%= f.submit "Login", class: "btn btn-default" %>
          </div>
        </div>
        <% end %>

      </div>
    </div>
  </div>
</div>